{
  "name": "rollup-demo",
  "version": "1.0.0",
  "description": "rollup demo",
  "author": "Liviu Taban <tabanliviu@yahoo.com>",
  "license": "ISC",
  "scripts": {
    "test": "karma start karma.config.js",
    "babel": "babel ./src/js --out-dir dist/src/js --source-maps --plugins=transform-runtime,transform-es2015-modules-commonjs",
    "babel:watch": "npm run babel -- --watch",
    "rollup-js": "rollup -c rollup.js.config.js",
    "typescript": "tsc",
    "typescript:watch": "npm run typescript -- --watch",
    "rollup-ts": "rollup -c rollup.ts.config.js",
    "typedoc": "typedoc ./src/ts --out docs --mode file"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-plugin-external-helpers": "6.18.0",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "core-js": "2.4.1",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-babel-preprocessor": "6.0.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.1.0",
    "karma-rollup-plugin": "0.2.4",
    "rollup": "0.41.1",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-typescript": "0.8.1",
    "systemjs": "0.19.41",
    "typedoc": "0.5.3",
    "typedoc-default-themes": "0.4.0",
    "typescript": "2.1.4"
  }
}
